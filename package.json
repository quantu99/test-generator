{
  "name": "@callmequantu/test-generator",
  "version": "1.0.0",
  "description": "Auto-generate unit tests from source files with test flow comments",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "test-gen": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "test",
    "unit-test",
    "generator",
    "jest",
    "vitest",
    "automation",
    "typescript"
  ],
  "author": "PaulyDev",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/quantu99/test-generator"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@jest/globals": "^29.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}