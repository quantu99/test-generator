{
  "framework": "jest",
  "style": "strict",
  "includeComments": true,
  "generateMocks": false,
  "testEach": false,
  "outputPattern": "{name}.test{ext}",
  "ignorePatterns": [
    "**/*.d.ts",
    "**/node_modules/**"
  ],
  "typeResolution": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
